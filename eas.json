{
	"cli": {
		"version": ">= 5.2.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_API_URL": "https://zitwasteapi.azurewebsites.net"
			},
			"channel": "development",
			"autoIncrement": true,
			"android": {
				"buildType": "apk"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_API_URL": "https://zitwasteapi.azurewebsites.net"
			},
			"channel": "preview",
			"autoIncrement": true,
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"env": {
				"EXPO_PUBLIC_API_URL": "https://zitwasteapi.azurewebsites.net"
			},
			"channel": "production",
			"autoIncrement": true,
			"android": {
				"buildType": "apk"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
